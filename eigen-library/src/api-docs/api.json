    "paths": {
        "/api/v1/book": {
            "get": {
                "tags": [
                    "Endpoints"
                ],
                "summary": "Book List",
                "parameters": [
                    {
                        "name": "logsid",
                        "in": "path",
                        "description": "Log ID for viewing selected log history", 
                        "required": false
                    },
                    {
                        "name": "logtype",
                        "in": "query",
                        "description": "Logging type such as Error, Warning, Info and Debug",
                        "default": "error",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": ["error", "debug", "info", "warning"]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/GetLogsHistory"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "client_id": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Endpoints"
                ],
                "summary": "Storing any logs dumped from 5log SDK or other client",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/definitions/errorPayload"
                            }
                        }
                    },
                    "required": true
                },
                "security": [
                    {
                        "client_id": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/definitions/postResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
